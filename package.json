    "prepare": "husky install",
    "test": "max test",
    "test-air": "max test /pages/air",
    "test:coverage": "max test --coverage",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "precommit": "lint-staged",
    "pretty-quick": "pretty-quick --staged",
    "import-swagger": "sisyphus",
    "previte": "node scripts/addStyle.js",
    "vite": "cross-env PORT=7011 vite",
    "sisyphus": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 sisyphus",
