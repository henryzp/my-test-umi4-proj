  "version": "1.0.0",
  "scripts": {
    "commit": "git-cz",
    "commit-all": "git add --all && git-cz",
    "dev": "cross-env PORT=7011 NODE_OPTIONS=--max_old_space_size=2048 SOCKET_SERVER=http://localhost:7011 max dev",
    "start": "cross-env PORT=7011 HMR=none NODE_OPTIONS=--max_old_space_size=2048 BROWSERSLIST_IGNORE_OLD_DATA=1 max dev",
